{
    "name": "my-app",
    "private": true,
    "version": "1.0.0",
    "description": "My App",
    "main": "resources/repair/src/js/app.js",
    "repository": "",
    "license": "UNLICENSED",
    "scripts": {
        "start": "npm run dev",
        "dev": "cross-env NODE_ENV=development vite",
        "build": "cross-env NODE_ENV=production vite build && npx workbox generateSW workbox-config.js",
        "ios": "cross-env NODE_ENV=production vite build && npx cap copy ios",
        "android": "cross-env NODE_ENV=production vite build && npx cap copy android",
        "postinstall": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/fonts/",
        "host": "cross-env NODE_ENV=development vite --host"
    },
    "browserslist": [
        "IOS >= 15",
        "Safari >= 15",
        "last 5 Chrome versions",
        "last 5 Firefox versions"
    ],
    "dependencies": {
        "@capacitor/android": "^5.5.1",
        "@capacitor/app": "^5.0.6",
        "@capacitor/browser": "^5.1.0",
        "@capacitor/core": "^5.5.1",
        "@capacitor/ios": "^5.5.1",
        "@capacitor/keyboard": "^5.0.6",
        "@capacitor/splash-screen": "^5.0.6",
        "@capacitor/status-bar": "^5.0.6",
        "@mdx-js/rollup": "^3.0.0",
        "@vitejs/plugin-react-refresh": "^1.3.6",
        "compressorjs": "^1.2.1",
        "dom7": "^4.0.6",
        "dotenv": "^16.3.1",
        "framework7": "^8.3.0",
        "framework7-icons": "^5.0.5",
        "framework7-react": "^8.3.0",
        "hamburger-react": "^2.5.0",
        "json-loader": "^0.5.7",
        "laravel-vite-plugin": "^0.8.1",
        "material-icons": "^1.13.12",
        "primereact": "^10.2.1",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-echo": "^1.0.12",
        "react-google-fonts": "^1.2.1",
        "react-moving-text": "^0.0.7",
        "react-pusher": "^0.2.0",
        "react-query": "^3.39.3",
        "react-toastify": "^9.1.3",
        "rsuite": "^5.45.0",
